{
  "name": "backend-grd",
  "version": "1.0.0",
  "description": "Backend para sistema GRD de UC Christus (exportación e importación de datos)",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "grd",
    "uc-christus",
    "backend",
    "excel",
    "export",
    "fonasa",
    "csv",
    "prisma",
    "typescript"
  ],
  "author": "UC Christus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taller-uc-christus-grd/backend-grd.git"
  },
  "bugs": {
    "url": "https://github.com/taller-uc-christus-grd/backend-grd/issues"
  },
  "homepage": "https://github.com/taller-uc-christus-grd/backend-grd#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.9.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.18.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
